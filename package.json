{
  "name": "unplugin-turbo-console",
  "type": "module",
  "version": "1.5.1",
  "packageManager": "pnpm@8.6.12",
  "description": "Improve the Developer Experience of console.log()",
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-turbo-console#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-turbo-console.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-turbo-console/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vue3-cli": "pnpm run --filter vue3-cli serve",
    "play:svelte-kit": "pnpm run --filter svelte-kit dev",
    "play:nuxt": "pnpm run --filter nuxt-app dev",
    "play:vite-vue3": "pnpm run --filter vite-vue3 dev",
    "play:next": "pnpm run --filter nextjs dev",
    "play:rspack": "pnpm run --filter rspack-vue dev",
    "play:astro": "pnpm run --filter astro dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "astro": ">=3",
    "esbuild": "*",
    "rollup": ">=3",
    "vite": ">=3",
    "vue": ">=2.7 || >=3.2.13",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "ast-kit": "^0.12.1",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "launch-editor": "^2.6.1",
    "magic-string": "^0.30.9",
    "pathe": "^1.1.2",
    "source-map-js": "^1.2.0",
    "unplugin": "^1.10.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.1",
    "@babel/types": "^7.24.0",
    "@nuxt/kit": "^3.11.1",
    "@nuxt/schema": "^3.11.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.4",
    "bumpp": "^9.4.0",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "webpack": "^5.91.0"
  }
}
