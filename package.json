{
  "name": "unplugin-turbo-console",
  "type": "module",
  "version": "1.11.3",
  "packageManager": "pnpm@10.5.2",
  "description": "Improve the Developer Experience of console",
  "license": "MIT",
  "homepage": "https://utc.yuy1n.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-turbo-console.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-turbo-console/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./helper": {
      "types": "./dist/helper.d.ts",
      "import": "./dist/helper.mjs",
      "require": "./dist/helper.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "unbuild && pnpm run build:client",
    "build:client": "pnpm run --filter client build",
    "dev": "unbuild --watch",
    "dev:client": "pnpm run --filter client dev",
    "build:fix": "tsx scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vue3-cli": "pnpm run --filter vue3-cli serve",
    "play:svelte": "pnpm run --filter svelte dev",
    "play:nuxt": "pnpm run --filter nuxt-app dev",
    "play:vite-vue3": "pnpm run --filter vite-vue3 dev",
    "play:next": "pnpm run --filter nextjs dev",
    "play:rspack": "pnpm run --filter rspack-vue dev",
    "play:astro": "pnpm run --filter astro dev",
    "play:farm": "pnpm run --filter farm-vue dev",
    "play:solid": "pnpm run --filter solid-start dev",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:preview": "pnpm run -C ./docs preview",
    "docs:build": "pnpm run -C ./docs build",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "astro": ">=3",
    "esbuild": "*",
    "rollup": ">=3",
    "vite": ">=3",
    "vue": ">=3",
    "webpack": ">=5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "estree-walker": "^3.0.3",
    "get-port-please": "^3.1.2",
    "h3": "1.12.0",
    "launch-editor": "^2.9.1",
    "magic-string": "^0.30.17",
    "mrmime": "^2.0.1",
    "oxc-parser": "^0.48.1",
    "pathe": "^2.0.2",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@nuxt/kit": "^3.15.3",
    "@nuxt/schema": "^3.15.3",
    "@oxc-project/types": "^0.48.1",
    "@tailwindcss/vite": "^4.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.10",
    "@vitest/ui": "^3.0.4",
    "bumpp": "^10.0.1",
    "chalk": "^5.4.1",
    "eslint": "^9.19.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.32.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "3.3.1",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vitest": "^3.0.4",
    "vue": "^3.5.13",
    "webpack": "^5.97.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sveltejs/kit",
      "esbuild",
      "sharp",
      "vue-demi"
    ]
  },
  "overrides": {
    "crossws": "0.2.4"
  }
}
