{
  "name": "unplugin-turbo-console",
  "type": "module",
  "version": "2.3.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Improve the Developer Experience of console",
  "license": "MIT",
  "homepage": "https://utc.yuy1n.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-turbo-console.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-turbo-console/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./helper": {
      "types": "./dist/helper.d.ts",
      "import": "./dist/helper.js",
      "require": "./dist/helper.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "build:client": "pnpm run --filter client build",
    "dev": "tsdown --watch",
    "dev:client": "pnpm run --filter client dev",
    "lint": "eslint .",
    "play:vite-vue3": "pnpm run --filter vite-vue3 dev",
    "play:nuxt": "pnpm run --filter nuxt-app dev",
    "play:vue3-cli": "pnpm run --filter vue3-cli serve",
    "play:svelte": "pnpm run --filter svelte dev",
    "play:next": "pnpm run --filter nextjs dev",
    "play:rspack": "pnpm run --filter rspack-vue dev",
    "play:astro": "pnpm run --filter astro dev",
    "play:farm": "pnpm run --filter farm-vue dev",
    "play:solid": "pnpm run --filter solid-start dev",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:preview": "pnpm run -C ./docs preview",
    "docs:build": "pnpm run -C ./docs build",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "astro": ">=3",
    "esbuild": "*",
    "rollup": ">=3",
    "vite": ">=3",
    "vue": ">=3",
    "webpack": ">=5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "alien-signals": "^2.0.6",
    "estree-walker": "^3.0.3",
    "get-port-please": "^3.2.0",
    "h3": "^1.15.4",
    "launch-editor": "^2.11.1",
    "magic-string": "^0.30.17",
    "mrmime": "^2.0.1",
    "oxc-parser": "^0.82.1",
    "pathe": "^2.0.3",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@nuxt/kit": "^4.0.3",
    "@nuxt/schema": "^4.0.3",
    "@oxc-project/types": "^0.82.1",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.2.1",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "rollup": "^4.46.2",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "webpack": "^5.101.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sveltejs/kit",
      "esbuild",
      "sharp",
      "vue-demi",
      "svelte-preprocess"
    ]
  }
}
