{
  "name": "vite-plugin-turbo-console",
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@8.1.1",
  "description": "ðŸš€ Enhance the ability of console.log()",
  "author": "yuyinws",
  "license": "MIT",
  "homepage": "https://github.com/yuyinws/vite-plugin-turbo-console#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyinws/vite-plugin-turbo-console.git"
  },
  "bugs": "https://github.com/yuyinws/vite-plugin-turbo-console/issues",
  "keywords": [
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub && nr play",
    "lint": "eslint .",
    "play": "vite playground --port 5254",
    "dev:nuxt": "unbuild --stub && pnpm run --filter ./examples/nuxt dev",
    "dev:svelte": "unbuild --stub && pnpm run --filter ./examples/svelte-kit dev",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^3.0.0 || ^5.0.0-0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "acorn-walk": "^8.2.0",
    "magic-string": "^0.30.0",
    "sirv": "^2.0.3",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@antfu/ni": "^0.21.2",
    "@antfu/utils": "^0.7.2",
    "@nuxt/kit": "^3.8.0",
    "@nuxt/schema": "^3.8.0",
    "@types/fs-extra": "^11.0.3",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.53.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.3",
    "unbuild": "^2.0.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
