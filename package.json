{
  "name": "unplugin-turbo-console",
  "type": "module",
  "version": "1.9.0-beta.5",
  "description": "Improve the Developer Experience of console.log()",
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-turbo-console#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-turbo-console.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-turbo-console/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch src",
    "build:fix": "tsx scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vue3-cli": "pnpm run --filter vue3-cli serve",
    "play:svelte": "pnpm run --filter svelte dev",
    "play:nuxt": "pnpm run --filter nuxt-app dev",
    "play:vite-vue3": "pnpm run --filter vite-vue3 dev",
    "play:next": "pnpm run --filter nextjs dev",
    "play:rspack": "pnpm run --filter rspack-vue dev",
    "play:astro": "pnpm run --filter astro dev",
    "play:farm": "pnpm run --filter farm-vue dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "astro": ">=3",
    "esbuild": "*",
    "rollup": ">=3",
    "vite": ">=3",
    "vue": ">=2.7 || >=3.2.13",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "ast-kit": "^1.0.0",
    "get-port-please": "^3.1.2",
    "h3": "^1.12.0",
    "launch-editor": "^2.8.0",
    "magic-string": "^0.30.10",
    "pathe": "^1.1.2",
    "unplugin": "^1.11.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.3",
    "@babel/types": "^7.24.7",
    "@nuxt/kit": "^3.12.3",
    "@nuxt/schema": "^3.12.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "@vitest/ui": "^2.0.1",
    "bumpp": "^9.4.1",
    "chalk": "^5.3.0",
    "eslint": "^9.6.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.18.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-inspect": "^0.8.4",
    "vitest": "^2.0.1",
    "vue": "^3.4.31",
    "webpack": "^5.92.1"
  }
}
